{
  "borderNone": true,
  "brackets": false,
  "colons": true,
  "colors": true,
  "commaSpace": true,
  "commentSpace": true,
  "cssLiteral": false,
  "depthLimit": 5,
  "duplicates": true,
  "efficient": true,
  "emoji": true,
  "enforceBlockStyle": true,
  "enforceVarStyle": true,
  "extendPref": false,
  "globalDupe": true,
  "indentSpaces": 2,
  "leadingZero": true,
  "maxWarnings": 10,
  "maxWarningsKill": false,
  "mixed": true,
  "namingConvention": "BEM",
  "namingConventionStrict": true,
  "parenSpace": false,
  "placeholders": true,
  "quotePref": "single",
  "stackedProperties": false,
  "semicolons": true,
  "sortOrder": "alphabetical",
  "universal": true,
  "valid": true,
  "whitespace": true,
  "zeroUnits": true,
  "zIndexDuplicates": true,
  "zIndexNormalize": 5
}